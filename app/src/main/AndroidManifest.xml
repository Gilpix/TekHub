<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kulartist.tekhubandroid">

    <application
        android:allowBackup="true"
        android:icon="@drawable/tehublogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tehublogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.actions"
            android:resource="@anim/fab_close" />

        <activity android:name="com.kulartist.tekhub.Item" />
        <activity android:name="com.kulartist.tekhub.ItemList" />
        <activity android:name="com.kulartist.tekhub.Student" />
        <activity android:name="com.kulartist.tekhub.BottomMenu" />
        <activity android:name=".ResetPassword" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegistrationActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
    </application>

</manifest>